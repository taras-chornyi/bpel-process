<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:bp="https://github.com/taras-chornyi/bpel-process">
    <process name="bp:BusinessProcess">
        <active>true</active>
        <retired>false</retired>
        <process-events generate="all"/>
        <provide partnerLink="client">
            <service name="bp:BusinessProcessService" port="BusinessProcessPort"/>
        </provide>
    </process>
</deploy>